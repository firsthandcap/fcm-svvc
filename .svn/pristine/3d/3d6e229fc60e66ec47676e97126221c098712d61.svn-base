/*********************************************************************************/
/* Tables                                                                         */
/*********************************************************************************/


	.table-wrapper {
		-webkit-overflow-scrolling: touch;
		overflow-x: auto;
	}

	table {
		margin: 0 0 2em 0;
	}

		table tbody tr {
			border-left: 0;
			border-right: 0;
		}

		table td {
			padding: 0.75em 0.75em;
			line-height: 1.5em;
		}
		
		table td:first-child {
			vertical-align: top;}
		
		table td a{
			color: #353535;
			text-decoration: none;
		}
