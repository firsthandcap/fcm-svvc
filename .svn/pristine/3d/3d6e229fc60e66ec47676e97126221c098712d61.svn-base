ul.stylebullets 	{
padding-top: 1em;
list-style-type: disc;

}

ul.stylebullets > li	{
position: relative;
padding: 0.7em 0 0.7em 0em;
line-height: 1.5em;
}

ul.stylebullets > li:before {
position: absolute;
left: 0;
top: 0.35em;
display: block;
color: #3a71d8;
width: 30px;
height: 30px;
line-height: 30px;
text-align: center;
font-size: 1.4em;
}

ul.stylebullets > li:first-child {	
padding-top: 15px;
}

ul.stylebullets > li:first-child:before {
top: 0;
}

ul.stylebullets > li:last-child {
padding: 0.7em 0 0.7em 0em;
}

ul.stylebullets a {
text-decoration: none;
color: #CC4A14;
}

ul.stylebullets a:hover {
text-decoration: underline;

}

